generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Income {
  id       String   @id
  category String
  amount   Float
  source   String
  date     DateTime @default(now())
}

model User {
  id       String @id
  email    String @unique
  password String
}
